    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@types/next-redux-wrapper": "^3.0.0",
